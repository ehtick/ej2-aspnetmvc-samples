@section Meta{
                    <meta name="description" content="This example demonstrates the Default Functionalities in ASP.NET MVC Ribbon control. Explore here for more details."/>
                }
@section ActionDescription{
    <div id="action-description">
        <p>This sample showcases the basic ribbon features with all its item types.</p>
    </div>
}
