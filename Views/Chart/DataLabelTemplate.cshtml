@using Syncfusion.EJ2;
@section ControlsSection{
    <div class="control-section" style="text-align:center">
        @Html.EJS().Chart("container").Series(series =>
        {
            series.Type(Syncfusion.EJ2.Charts.ChartSeriesType.Column).
                Name("Boys").
                XName("Sports").
                YName("Boys").ColumnWidth(0.75).ColumnSpacing(0.5).
                Marker(mr => mr.Visible(false).Width(10).Height(10).Shape(Syncfusion.EJ2.Charts.ChartShape.Circle).DataLabel(dl => dl.Visible(true).Position(Syncfusion.EJ2.Charts.LabelPosition.Outer).Margin(mar => mar.Top(70)).Template("#Boys-Material"))).
                DataSource(ViewData["PopulationDetails"]).
                Add();

            series.Type(Syncfusion.EJ2.Charts.ChartSeriesType.Column).
                Name("Girls").
                XName("Sports").
                YName("Girls").ColumnWidth(0.75).ColumnSpacing(0.5).
                Marker(mr => mr.Visible(false).Width(10).Height(10).Shape(Syncfusion.EJ2.Charts.ChartShape.Rectangle).DataLabel(dl => dl.Visible(true).Position(Syncfusion.EJ2.Charts.LabelPosition.Outer).Margin(mar => mar.Top(70)).Template("#Girls-Material"))).
                DataSource(ViewData["PopulationDetails"])
                .Add();

        }).PrimaryXAxis(
            xAxis =>
            {
                xAxis.MajorTickLines(mt => mt.Width(0))
                    .ValueType(Syncfusion.EJ2.Charts.ValueType.Category).
                    EdgeLabelPlacement(Syncfusion.EJ2.Charts.EdgeLabelPlacement.Shift).
                    MajorGridLines(mg => mg.Width(0));
            }).PrimaryYAxis(
                yAxis =>
                {
                    yAxis.
                        Minimum(0).
                        Maximum(70).LineStyle(ls => ls.Width(0)).
                        MajorGridLines(mg => mg.Color("#eaeaea").Width(1)).MajorTickLines(mt => mt.Width(0));
                }).ChartArea(area => area.Border(br => br.Width(0))).LegendSettings(lg => lg.EnableHighlight(true)
            ).Title("Athletes in Popular School").SubTitleStyle(st => st.TextAlignment(Syncfusion.EJ2.Charts.Alignment.Far)).Load("load").TextRender("textRender").Render()

    </div>


    <style>
        #control-container {
            padding: 0px !important;
        }

        #templateWrap img {
            border-radius: 0px;
            width: 24px;
            padding: 4px;
            height: 24px;
        }

        #templateWrap .des {
            float: right;
            padding: 2px;
            line-height: 20px;
            text-align: center;
        }
    </style>
